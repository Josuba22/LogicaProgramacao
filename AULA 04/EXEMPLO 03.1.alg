algoritmo "Estrutura SE juntamente do SENAO"

var x,y: inteiro

inicio

    escreva("Digite o primeiro valor: ")
    leia(x)
    escreval(x)

    escreva("Digite o segundo valor: ")
    leia(y)
    escreval(y)

    se x <> y faca
        escreva("O primeiro número é diferente do segundo número")
    senao
        escreva("O primeiro e segundo número são iguais")
    fimse

fimalgoritmo