algoritmo "Operação Matemática 03"

var n1: inteiro
    n2: inteiro
    result: inteiro

inicio


    escreval("Insira o 1º valor: ")
    leia(n1)
    escreval("Insira o 2º valor: ")
    leia(n2)
    result <- n1 * n2
    escreva("O resultado da multiplicação entre o 1º e o 2º valor é: ", result)


fimalgoritmo